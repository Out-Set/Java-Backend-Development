	<subDataset name="New Dataset 1" uuid="0953bcb3-5a0b-456d-8b23-af420d4a951b">
		<parameter name="applicationNumber" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[(SELECT
(SELECT P.FULL_NAME FROM PERSON_INFO P WHERE P.ID = CUS.PERSON_INFO) "CUSTOMER NAME",
CONCAT((SELECT CODE FROM GENERIC_PARAMETER_FIS WHERE DTYPE = 'CASURL'),CONCAT('creditBureau/bureauHistoryReport/',
(MAX(CB.DETAILED_REPORTXMLSTORE_ID)))) "CIBIL"
FROM CREDIT_BUREAU_EXCHANGE_RECORD CB,CUSTOMER CUS
WHERE CB.CUSTOMER_ID = CUS.ID AND CB.DETAILED_REPORTXMLSTORE_ID IS NOT NULL AND  CUS.ID IN (
SELECT P.CUSTOMER FROM PARTY P WHERE P.LOAN_APPLICATION_FK = (
SELECT LA.ID FROM LOAN_APPLICATION LA WHERE LA.APPLICATION_NUMBER = ($P{applicationNumber})))
and TRUNC(CB.CREATION_TIME_STAMP) < '15-Oct-2017' GROUP BY (CB.CUSTOMER_ID,CUS.PERSON_INFO) union
SELECT
(SELECT P.FULL_NAME FROM PERSON_INFO P WHERE P.ID = CUS.PERSON_INFO) "CUSTOMER NAME",
CONCAT((SELECT CODE FROM GENERIC_PARAMETER_FIS WHERE DTYPE = 'CASURL'),CONCAT('creditBureau/downloadPdfReport/',
(MAX(CB.DETAILED_REPORTPDFSTORE_ID)))) "CIBIL"
FROM CREDIT_BUREAU_EXCHANGE_RECORD CB,CUSTOMER CUS
WHERE CB.CUSTOMER_ID = CUS.ID AND CB.DETAILED_REPORTPDFSTORE_ID IS NOT NULL AND  CUS.ID IN (
SELECT P.CUSTOMER FROM PARTY P WHERE P.LOAN_APPLICATION_FK = (
SELECT LA.ID FROM LOAN_APPLICATION LA WHERE LA.APPLICATION_NUMBER = ($P{applicationNumber})))
and TRUNC(CB.CREATION_TIME_STAMP) >= '15-Oct-2017' GROUP BY (CB.CUSTOMER_ID,CUS.PERSON_INFO))]]>
		</queryString>
		<field name="CUSTOMER NAME" class="java.lang.String"/>
		<field name="CIBIL" class="java.lang.String"/>
	</subDataset>
	<subDataset name="New Dataset 2" uuid="7d85e8c6-c191-484e-930f-06b22d5e27a6">
		<parameter name="applicationNumber" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
(SELECT PI.FULL_NAME FROM PERSON_INFO PI WHERE PI.ID = (
SELECT CUST.PERSON_INFO FROM CUSTOMER CUST WHERE CUST.ID = P.CUSTOMER)) "CUSTOMER NAME",
(SELECT GP.NAME FROM GENERIC_PARAMETER GP WHERE GP.ID = FIE.TYPE) "TYPE",
(SELECT BP.NAME FROM BUSINESS_PARTNER BP WHERE BP.ID = (
SELECT AG.ID FROM AGENCY AG WHERE AG.ID = FIE.VERIFICATION_AGENCY)) "AGENCY",
(SELECT BP.NAME FROM BUSINESS_PARTNER BP WHERE BP.ID = (
SELECT AT.ID FROM AGENT AT WHERE AT.ID = FIE.VERIFICATION_AGENT)) "AGENT",
(CONCAT((SELECT CODE FROM GENERIC_PARAMETER_FIS WHERE DTYPE = 'CASURL'),
CONCAT('FieldInvestigation/FieldInvestigationVerification/getAttachedDocument/',
D.DOCUMENT_STORE_ID))) "FIV"
FROM
DOCUMENT D,FIELD_INVESTIGATION_ENTRY FIE,
FIELD_INVESTIGATION FI,PARTY P
WHERE
D.FK_FIELD_INVESTIGATION_ENTRY = FIE.ID
AND
FIE.FIELD_INVESTIGATION_FK = FI.ID AND FI.ID IN (P.FIELD_INVESTIGATION)
AND
P.LOAN_APPLICATION_FK = (
SELECT ID FROM LOAN_APPLICATION WHERE APPLICATION_NUMBER = ($P{applicationNumber}))]]>
		</queryString>
		<field name="CUSTOMER NAME" class="java.lang.String"/>
		<field name="TYPE" class="java.lang.String"/>
		<field name="AGENCY" class="java.lang.String"/>
		<field name="AGENT" class="java.lang.String"/>
		<field name="FIV" class="java.lang.String"/>
	</subDataset>
	<subDataset name="New Dataset 3" uuid="3a42759f-ba12-4be7-b3cf-bfbc9c1f2ef7">
		<parameter name="applicationNumber" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[(SELECT
LA.APPLICATION_NUMBER "APPLICATION NUMBER",
R.RCU_STATUS "RCU STATUS",
(SELECT BP.NAME FROM BUSINESS_PARTNER BP WHERE BP.ID = (
SELECT AG.ID FROM AGENCY AG WHERE AG.ID = R.RCU_AGENCY)) "AGENCY",
(CONCAT((SELECT CODE FROM GENERIC_PARAMETER_FIS WHERE DTYPE = 'CASURL'),
CONCAT('LoanApplication/RCU/showAttachment/',
D.DOCUMENT_STORE_ID))) "RCU"
FROM
DOCUMENT D,JT_RCU_DOCUMENT JRD,
RCUVERDICT R,LOAN_APPLICATION LA
WHERE
D.ID = JRD.ATTACHED_DOCUMENT_LIST
AND
R.ID = JRD.RCUVERDICT
AND
R.ID = LA.APPLICATIONRCUVERDICT
AND
LA.APPLICATION_NUMBER = ($P{applicationNumber}))]]>
		</queryString>
		<field name="APPLICATION NUMBER" class="java.lang.String"/>
		<field name="RCU STATUS" class="java.lang.String"/>
		<field name="AGENCY" class="java.lang.String"/>
		<field name="RCU" class="java.lang.String"/>
	</subDataset>
	<parameter name="appNo" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
(SELECT LA.APPLICATION_NUMBER FROM LOAN_APPLICATION LA WHERE LA.APPLICATION_NUMBER = ($P{appNo})) "APPLICATION NUMBER",
(SELECT PI.FULL_NAME FROM PERSON_INFO PI WHERE PI.ID = (
SELECT CUST.PERSON_INFO FROM CUSTOMER CUST WHERE CUST.ID = (
SELECT P.CUSTOMER FROM PARTY P WHERE P.PARTY_ROLE = '0' AND P.LOAN_APPLICATION_FK = (
SELECT LA.ID FROM LOAN_APPLICATION LA WHERE LA.APPLICATION_NUMBER = ($P{appNo}))))) "CUSTOMER NAME",
((SELECT CODE FROM GENERIC_PARAMETER_FIS WHERE DTYPE = 'CASURL')||'camReport/core/homeLoan/'||
(SELECT LA.ID FROM LOAN_APPLICATION LA WHERE LA.APPLICATION_NUMBER = ($P{appNo}))) "CAM",
CASE (SELECT NVL(SL.LOAN_ACCOUNT_NUMBER,'NULL') FROM LOAN_APPLICATION LA,SUB_LOAN SL WHERE LA.ID = SL.APPLICATION_FK
AND APPLICATION_NUMBER = ($P{appNo})) WHEN 'NULL'
THEN ((SELECT CODE FROM GENERIC_PARAMETER_FIS WHERE DTYPE = 'CASURL')||'LoanApplication/dcb/generateSanctionLetter?appId='||
(SELECT ID FROM LOAN_APPLICATION WHERE APPLICATION_NUMBER = ($P{appNo})))
ELSE (SELECT G.CODE||'lead/getAttachedDocument/'|| O.ID AS
FROM ORACLE_DOCUMENT_ENTITY O,GENERIC_PARAMETER_FIS G WHERE G.DTYPE = 'CASURL'
AND O.ID IN (
SELECT MAX(DOCUMENT_STORE_ID) FROM DOCUMENT WHERE COMMUNICATION_FK IN (
SELECT ID FROM COMMUNICATION_TRAIL WHERE COMMUNICATION_FK IN (
SELECT ID FROM COMMUNICATION WHERE OWNER_ENTITY_URI IN ('com.nucleus.core.loan.LoanApplication:'||
(SELECT ID FROM LOAN_APPLICATION WHERE APPLICATION_NUMBER = ($P{appNo}))))))) END "SANCTION"
FROM DUAL]]>
	</queryString>
	<field name="APPLICATION NUMBER" class="java.lang.String"/>
	<field name="CUSTOMER NAME" class="java.lang.String"/>
	<field name="CAM" class="java.lang.String"/>
	<field name="SANCTION" class="java.lang.String"/>
